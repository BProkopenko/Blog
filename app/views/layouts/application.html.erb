<%= render "layouts/head" %>
<body>
<%= render "layouts/header" %>
<div id="content">
  <div id="flashMessages">
    <% flash.each do |message_type, message| %>
      <div class="alert alert-<%= message_type %>"><%= message %></div>
    <% end %>
  </div>
  <%= yield %>
</div>
<%= render "layouts/footer" %>
<%= render "layouts/debug" %>
</body>
</html>
